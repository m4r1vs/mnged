{
  "name": "mnged",
  "version": "0.3.1",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "if-env NODE_ENV=production && yarn run -s serve || yarn run -s dev",
    "build": "preact build --template src/template.html",
    "deploy": "yarn build && firebase deploy --only hosting",
    "serve": "preact build --template src/template.html && preact serve",
    "dev": "preact watch --template src/template.html -p 2211",
    "test": "eslint src && preact test"
  },
  "eslintConfig": {
    "extends": "eslint-config-synacor"
  },
  "devDependencies": {
    "eslint": "^4.8.0",
    "eslint-config-synacor": "^2.0.2",
    "if-env": "^1.0.0",
    "node-sass": "^4.5.3",
    "preact-cli": "^1.4.1",
    "preact-cli-sw-precache": "^1.0.3",
    "sass-loader": "^6.0.6",
    "webpack-bundle-analyzer": "^2.9.0"
  },
  "dependencies": {
    "firebase": "4.6.1",
    "mobx": "^3.3.1",
    "mobx-logger": "^0.6.0",
    "preact": "^8.2.5",
    "preact-compat": "^3.17.0",
    "preact-mobx": "^0.1.4",
    "preact-router": "^2.5.7",
    "xmlhttprequest": "^1.8.0"
  }
}
